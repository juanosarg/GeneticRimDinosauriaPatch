<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ========= Spinobear ========= -->
	<ThingDef ParentName="EggUnfertBase">
		<defName>GR_SpinobearEggUnfertilized</defName>
		<label>spinobear egg (unfert.)</label>
		<description>Unfertilized spinobear egg. It can be eaten raw, but it's much, much better cooked.</description>
		<tradeability>Sellable</tradeability>
		<graphicData>
			<texPath>Things/Item/GR_DinoEgg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>.7</drawSize>
		</graphicData>
		<statBases>
			<MarketValue>20</MarketValue>

		</statBases>

	</ThingDef>

	<ThingDef ParentName="EggFertBase">
		<defName>GR_SpinobearEggFertilized</defName>
		<label>spinobear egg (fert.)</label>
		<description>Fertilized spinobear egg. It can be eaten raw, but it's much, much better cooked. If all goes well, it should hatch into an spinobear.</description>
		<tradeability>None</tradeability>
		<graphicData>
			<texPath>Things/Item/GR_DinoEgg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>.7</drawSize>
		</graphicData>
		<statBases>
			<MarketValue>20</MarketValue>
			<Nutrition>.8</Nutrition>

		</statBases>

		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>15</hatcherDaystoHatch>
				<hatcherPawn>GR_Spinobear</hatcherPawn>
			</li>
		</comps>
	</ThingDef>  

